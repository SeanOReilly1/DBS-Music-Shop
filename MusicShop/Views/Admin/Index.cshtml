
@{
    ViewBag.Title = "Index";
}

@section logged{
    @if (Session == null || Session["userType"] == null || Session["userType"].ToString() != "staff")
    {
        Response.Redirect(Url.Action("Login", "Admin"));
    }
}

<h2>Index</h2>

<div class="form">


    <table>
        <thead>
            <tr>
                <th>Administrators</th>
                <th>Customers</th>
                <th>Orders</th>
                

            </tr>
        </thead>
        <tbody>




            <tr>
                <td>@Html.ActionLink("Admin Admins", "ListOfAdmin", "Admin")</td>
                <td>@Html.ActionLink("Admin Customers", "ListOfCustomer", "Customer")</td>
                <td>@Html.ActionLink("Admin Orders", "AdminOrders", "Order")</td>
            


            </tr>



        </tbody>
    </table>
    <hr />
</div>

